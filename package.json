{
  "name": "govio-planner-workspace",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start-govio-planner": "ng serve govio-planner --port 5203",
    "start-govio-planner-proxy": "ng serve govio-planner --proxy-config proxy.config.json --port 5203",
    "production-govio-planner": "node ./builder.version.js --project=govio-planner && npm run distribution-govio-planner",
    "distribution-govio-planner": "npm run build-govio-planner",
    "build-govio-planner": "ng build govio-planner --configuration production --aot=true --base-href /govio-planner-app/",
    "clean-govio-planner": "rm -r ./dist/govio-planner-app/assets/scss && rm -r ./dist/govio-planner-app/assets/*.js",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.4",
    "@angular/cdk": "^15.2.4",
